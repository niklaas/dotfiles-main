[user]
    name = Niklaas Baudet von Gersdorff
    email = me@niklaas.eu
[credential]
    helper = cache
[core]
    editor = vim
    autocrlf = false
    filemode = false
[merge]
    tool = vimdiff
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    last = log -1 HEAD
	pushall = !git remote | xargs -L1 git push --all
[push]
    default = current
[http]
    sslverify = true
